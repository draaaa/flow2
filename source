local Settings = FlowHub:addPage("settings", 5012544693)
local Colors = Settings:addSection("colors")

for theme, color in pairs(Themes) do
	Colors:addColorPicker(theme, color, function(color3)
		FlowHub:setTheme(theme, color3)
	end)
end
local Plus = Settings:addSection("menu")

Plus:addKeybind("toggle ui", Enum.KeyCode[ActualSettings.CloseUI], function()
	FlowHub:toggle()
end,function(Key)
    Settings.CloseUI = tostring(Key.KeyCode):gsub("%a+%.%a+%.","")
    ActualSettings.CloseUI = tostring(Key.KeyCode):gsub("%a+%.%a+%.","")
    Save()
end)


