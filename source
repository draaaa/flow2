local Time = Visuals:addSection("time")

Time:addToggle("day", nil, function(day)
	if day then
		Day = true
		while Day do
			task.wait()
			game.Lighting.ClockTime = 14
		end
	else
		Day = false
	end
end)

Time:addToggle("evening", nil, function(evening)
	if evening then
		Evening = true
		while Evening do
			task.wait()
			game.Lighting.ClockTime = 17.60
		end
	else
		Evening = false
	end
end)

Time:addToggle("night", nil, function(night)
	if night then
		Night = true
		while Night do
			task.wait()
			game.Lighting.ClockTime = 0
		end
	else
		Night = false
	end
end)

local Misc = Visuals:addSection("misc")

Misc:addSlider("fov", 70, 1, 120, function(FOV)
	workspace.CurrentCamera.FieldOfView = FOV
end)

Misc:addSlider("saturation", 0.2, -25, 25, function(Saturation)
	game.Lighting.ColorCorrection.Saturation = Saturation
end)


