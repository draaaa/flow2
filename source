local Exp = Exploits:addSection("exploits")

Exp:addToggle("noslow", nil, function(BNS)
	if BNS then
		BNSRun = RS.RenderStepped:Connect(function()
			pcall(function()
				for _, v in ipairs(Player.Character:GetChildren()) do
					if table.find(SlowValues, v.Name) then
						v:Destroy()
					end
					if v.Name == "Block" and v.Value then
						v.Value = false
					end
					if v:FindFirstChild("BodyVelocity") then
						v.BodyVelocity:Destroy()
					end
				end
			end)
		end)
	else
		if BNSRun then
			BNSRun:Disconnect()
		end
	end
end)

